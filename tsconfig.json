// Only used for eslint and intellisense for files outside package src folders
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "checkJs": true
  },
  "include": ["./*.js", "./*.mjs", "./*/*.js", "./packages/*/*.js"],
  "exclude": ["**/node_modules"]
}
